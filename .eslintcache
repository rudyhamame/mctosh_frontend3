y has null object\r\n      received_auth_report.is_authorized === true\r\n    ) {\r\n      // It will work because we save the initial value of the received report state to be the one int he browser\r\n      return <Route {...rest} render={() => children} />;\r\n    } else {\r\n      return <Redirect to=\"/\" />;\r\n    }\r\n  };\r\n\r\n  return (\r\n    <Router>\r\n      <PrivateRoute\r\n        exact\r\n        path=\"/\"\r\n        received_auth_report={received_auth_report} //passing the report from the Router to the App component\r\n        component={() => <App received_auth_report={received_auth_report} />}\r\n      />\r\n      {received_auth_report === null && (\r\n        <Route exact path=\"/\">\r\n          <Login auth_report_receiver={auth_report_receiver} />\r\n        </Route>\r\n      )}\r\n      <PrivateRoute exact path=\"/profile\" component={Profile} />\r\n    </Router>\r\n  );\r\n};\r\n\r\nexport default AppRouter;\r\n","C:\\Users\\Rudy\\Documents\\step1\\step1-frontend2\\src\\static_components\\Profile\\Profile.jsx",["113"],"import React from \"react\";\r\n\r\nimport Main from \"../Main\";\r\n\r\n//........Home Component...........\r\nconst Home = (props) => {\r\n  return <div id=\"app_page\" className=\"fc\"></div>;\r\n};\r\n\r\nexport default Home;\r\n","C:\\Users\\Rudy\\Documents\\step1\\step1-frontend2\\src\\static_components\\NotFound.jsx",[],["114","115"],"C:\\Users\\Rudy\\Documents\\step1\\step1-frontend2\\src\\static_components\\Data.jsx",["116","117"],"import React from \"react\";\r\nimport AppRouter from \"../AppRouter\";\r\n\r\nconst Data = (props) => {\r\n  return null;\r\n};\r\n\r\nexport default Data;\r\n",{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","severity":1,"message":"123","line":4,"column":8,"nodeType":"124","messageId":"125","endLine":4,"endColumn":12},{"ruleId":"122","severity":1,"message":"126","line":5,"column":17,"nodeType":"124","messageId":"125","endLine":5,"endColumn":23},{"r